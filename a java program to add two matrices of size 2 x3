// create a java program to add two matrices of size 2 x3 
public class Main{ 
 public static void main ( String args[]){
     
     int [][] array = { {1,2,3},{1,2,3}};
     int [][] array2 = { {1,2,3},{1,2,3}};
     for (int i = 0 ; i< 2; i++ ){
     int value = 0 ;
         for(int  j= 0 ; j<3; j++){
            value =array[i][j] + array2[i][j];
         System.out.println(value);
         }
     }
     
 }
}

